package sistema;

public abstract class SistemaCompanhia implements Sistema {		
	
	@Override
	public abstract void reservaPoltrona(String numPoltrona);
	
	@Override
	public abstract void mostrarOpcoes(String classe, int numPessoas, String posicao);
	
	public abstract void cancelaVoo(int numero);
	
	public abstract void cadastraVoo(int numero, String horarioIda, String horarioChegada, String cidadeEmbarque,
			double precoEconomica, double precoExecutiva, Aeronave aeronave);
	
}

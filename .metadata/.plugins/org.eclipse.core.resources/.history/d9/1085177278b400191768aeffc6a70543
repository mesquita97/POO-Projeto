package sistema;

import aeronaves.Aeronave;

public class AgendaVoos implements SistemaCompanhia{
	Voo[] voos = new Voo[10];

	public Voo[] getVoos() {
		return voos;
	}

	public Voo getVoo(int numero) {
		for(Voo voo: voos) {
			if(voo.getNumero()==numero) return voo;	
		}
		return null;
	}

